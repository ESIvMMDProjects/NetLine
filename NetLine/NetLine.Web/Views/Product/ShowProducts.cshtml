@model IEnumerable<Product>
@{
    ViewData["Title"] = "ShowProducts";
}

<div class="text-center">
    <h1 class="display-4">به فروشگاه ما خوش اومدید</h1>
    <hr />
    <div class="row">
        @{
            foreach (var item in Model)
            {
                <partial name="_BoxProduct" model="@item" />
            }
        }
    </div>
</div>
